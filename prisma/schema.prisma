datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model School {
  id        Int      @id @default(autoincrement())
  name      String
  address   String
  city      String
  state     String
  contact   String
  email     String   @unique
  imageUrl  String
  createdAt DateTime @default(now())
}
